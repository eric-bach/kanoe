{
  "name": "axela",
  "version": "0.1.0",
  "bin": {
    "axela": "bin/axela.js"
  },
  "scripts": {
    "build": "tsc",
    "deploy": "npx cdk deploy --all -c appName=axela -c envName=dev --require-approval never --profile bach-dev",
    "deploy-prod": "npm run build-prod --prefix ../frontend && npx cdk deploy --all -c appName=axela -c envName=prod --require-approval never --profile bach-prod",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.4",
    "aws-cdk": "2.131.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.131.0-alpha.0",
    "@cdklabs/generative-ai-cdk-constructs": "^0.1.79",
    "aws-cdk-lib": "2.131.0",
    "cdk-aws-lambda-powertools-layer": "^3.7.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
